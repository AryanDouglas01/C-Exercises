#include<stdio.h>

/* 7) Faça um programa que receba do usuário um vetor X com 10 posições. Em
 seguida deverão ser impressos o maior e o menor elemento desse vetor.*/

int main()
{
    int vetor[8] = {12,43,32,64,7,45,75,90};
    int maior;
    int menor;
    
    for(int i = 0; i < 8; i++)
    {
     maior = vetor[0];
     
     if(vetor[i] > maior)
      {
         maior = vetor[i];      
      }
    }
    
     printf("O maior valor é %d \n", maior);
     
     for(int i = 7; i >=0; i--)
    {
     menor = vetor[7];
     
     if(vetor[i] < menor)
      {
         menor = vetor[i];    
      }
    }
    
       printf("O menor valor é %d", menor);
       
    return 0;
}
