// Esse programa lê 4 valores do tipo float e depois exibe a médi desses 4 valores.

#include <stdlib.h>
#include <stdio.h>

int main(){
    
    float num1, num2, num3,num4;
    
    printf("Insira quatro números!");
    scanf("%f %f %f %f ", &num1,&num2,&num3,&num4);
    
    float media = (num1 + num2 + num3 + num4)/4 ;
    
    printf("A media desses números é : %f", media );
    
    return 0; 
} 
