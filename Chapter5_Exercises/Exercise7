/*  Elabore um programa que peça ao usuário para digitar 10
 valores. Some esses valores e apresente o resultado na tela.*/

#include <stdio.h>

int main() {
    
  int numero,soma = 0;
	printf("Insira 10 numeros\n ");
	
    for(int n = 10; n >= 0; n--)
	{
		soma = soma + numero;
		scanf("%d", &numero);
	}

	printf("A soma dos numeros digitados eh %d", soma);
	printf("\n");
    return 0;
}
