/* Faça um algoritmo que leia um número positivo e imprima
seus divisores. Exemplo: Os divisores do número 66 são: 1, 2,
3, 6, 11, 22, 33 e 66. */

#include <stdio.h>

int main()
{
    int numero;
    int divisor = 1; 
    
    printf("Insira um número: ");
    scanf("%d", &numero);
    printf("Os divisores de %d são: ", numero);
    
    for(; divisor<=numero; divisor++)
    {
        if(numero%divisor == 0)
        {
            printf("%d ", divisor);
            
        }
    }
    return 0;
}
